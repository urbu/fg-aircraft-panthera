<?xml version="1.0" ?>  
<PropertyList> 

  <path>alt.ac</path>
  
 
  <animation> 
		<type>material</type> 
		
		<object-name>altimiter</object-name>
		<object-name>arrow_alt_big</object-name>
		<object-name>arrow_alt_small</object-name>
		<object-name>alt_trim</object-name>
		
		<texture-prop>texture</texture-prop>  
		<texture>textures/instruments.png</texture>  
		
 </animation>
 
 <animation>
  <type>rotate</type>
  <object-name>arrow_alt_big</object-name>
  <property>instrumentation/altimeter/indicated-altitude-ft</property>
  <factor>0.36</factor>
  <center>
            <x-m>-0.09656</x-m>
            <y-m>-2.04410</y-m>
            <z-m>0.16518</z-m>
        </center>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>arrow_alt_small</object-name>
  <property>instrumentation/altimeter/indicated-altitude-ft</property>
  <factor>0.036</factor>
  <center>
            <x-m>-0.09656</x-m>
            <y-m>-2.04410</y-m>
            <z-m>0.16518</z-m>
        </center>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>
	
</PropertyList>